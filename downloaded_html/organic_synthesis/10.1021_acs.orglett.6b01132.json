idth: 470px; height: 450px;" bis_skin_checked="1"><div class="fs-page-wrapper" bis_skin_checked="1" style="width: 400px; height: 565px;"><canvas height="565" width="400" style="width: 400px; height: 565px;"></canvas><div class="fs-text-layer" bis_skin_checked="1"></div></div><div class="fs-page-wrapper" bis_skin_checked="1" style="width: 400px; height: 565px;"><canvas height="565" width="400" style="width: 400px; height: 565px;"></canvas><div class="fs-text-layer" bis_skin_checked="1"></div></div><div class="fs-page-wrapper" bis_skin_checked="1" style="width: 400px; height: 565px;"><div class="figshare-loader fs-loading fs-loading-layer" bis_skin_checked="1"><div class="fs-figshare-loader-holder" bis_skin_checked="1"><div class="fs-figshare-loader-message" bis_skin_checked="1"><div aria-hidden="true" class="fs-logo" bis_skin_checked="1"><svg aria-hidden="true" class="figshare-logo" focusable="false" height="36px" role="img" viewBox="0 0 14 14" width="36px"><style type="text/css">
